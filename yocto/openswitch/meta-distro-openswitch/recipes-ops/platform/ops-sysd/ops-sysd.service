[Unit]
Description=OpenSwitch System Daemon (ops-sysd)
After=ovsdb-server.service ops-init.service

[Service]
Type=simple
ExecStart=/sbin/ip netns exec nonet /usr/bin/ops-sysd -vSYSLOG:INFO
Restart=on-abort

[Install]
WantedBy=multi-user.target
