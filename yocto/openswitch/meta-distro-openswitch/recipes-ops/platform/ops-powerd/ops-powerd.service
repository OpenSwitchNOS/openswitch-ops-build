[Unit]
Description=OpenSwitch Power Supply Management Daemon
After=ovsdb-server.service ops-init.service

[Service]
Type=simple
ExecStart=/sbin/ip netns exec nonet /usr/bin/ops-powerd
Restart=on-abort

[Install]
WantedBy=multi-user.target
