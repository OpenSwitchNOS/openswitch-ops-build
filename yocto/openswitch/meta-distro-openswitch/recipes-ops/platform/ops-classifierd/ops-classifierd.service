[Unit]
Description=OpenSwitch Classifier Daemon
After=ovsdb-server.service ops-init.service

[Service]
Type=simple
ExecStart=/sbin/ip netns exec nonet /usr/bin/ops-classifierd -vSYSLOG:INFO
Restart=on-abort

[Install]
WantedBy=multi-user.target
