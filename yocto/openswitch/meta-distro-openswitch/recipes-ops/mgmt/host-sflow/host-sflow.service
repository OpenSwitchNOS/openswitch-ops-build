[Unit]
Description=Host sFlow Daemon
After=ovsdb-server.service ops-init.service

[Service]
Type=forking
ExecStart=/sbin/ip netns exec swns /etc/init.d/hsflowd start
ExecStop=/sbin/ip netns exec swns /etc/init.d/hsflowd stop
ExecReload=/sbin/ip netns exec swns /etc/init.d/hsflowd restart


[Install]
WantedBy=multi-user.target
