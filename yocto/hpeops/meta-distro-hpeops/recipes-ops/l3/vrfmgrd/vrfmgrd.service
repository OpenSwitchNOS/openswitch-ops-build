[Unit]
Description=VRF Manager Daemon
After=ovsdb-server.service ops-init.service

[Service]
Type=forking
CPUSchedulingPolicy=rr
CPUSchedulingPriority=3
ExecStartPre=/bin/rm -f /var/run/openvswitch/vrfmgrd.pid
ExecStart=/usr/sbin/vrfmgrd --detach --pidfile -vSYSLOG:INFO
PIDFile=/var/run/openvswitch/vrfmgrd.pid
Restart=on-abort

[Install]
WantedBy=multi-user.target
